var abc
#  test
abc = 1
yield abc + 1, 10
out = each x in ttt where x.np > 15
 var delta
 delta = x.end - x.start * 1 + 0
 var i
 i = 0
 while i < 2
  if i == 0
    yield d => delta, u => x.user, i => i
  else
    yield d => delta, u => x.user, i => i + 1 
  end
  i = i + 1
 end
end

out2 = seq y in out
 var s, n
 s = s + y.d
 n = n + 1
# debug n
 yield sum2 => s, avg2 => s / n, u => y.u
final
 yield sum2 => s + 1024, avg2 => s + 1024 / ( n + 1 ), u => 'final_user' 
end

out3 = seq z in out2
  var x
  x = x + z.sum2
  yield sum3 => x, avg3 => z.avg2, u => z.u
end

out4 = seq z in out3
  var y
  y = y + z.sum3
  yield sum4 => y, avg4 => z.avg3, u => z.u
end

print out4

